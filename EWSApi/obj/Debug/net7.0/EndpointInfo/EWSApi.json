          "nameEn",
          "nameSq",
          "nameSr",
          "phoneNumber",
          "settlementId",
          "shortNameEn",
          "shortNameSq",
          "shortNameSr",
          "status"
        ],
        "type": "object",
        "properties": {
          "healthInstitutionId": {
            "type": "integer",
            "format": "int32"
          },
          "healthInstitutionLevelId": {
            "type": "integer",
            "format": "int32"
          },
          "identificationNumber": {
            "type": "string"
          },
          "licenceNumber": {
            "type": "string"
          },
          "nameSq": {
            "type": "string"
          },
          "nameEn": {
            "type": "string"
          },
          "nameSr": {
            "type": "string"
          },
          "shortNameSq": {
            "type": "string"
          },
          "shortNameEn": {
            "type": "string"
          },
          "shortNameSr": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "minicipalityId": {
            "maximum": 38,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "settlementId": {
            "type": "integer",
            "format": "int32"
          },
          "address": {
            "type": "string"
